<div class="bgcolor-white pt1em pb1em" id="contents">  
<div id="main_cnt_wrapper">
  <div id="yjContentsBody">
    <div class="yjContainer">
      <div class="form_box">
        <h2>Order<span>registration</span></h2>

        <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
          <%= devise_error_messages! %>

          <div class="label"><%= f.label :company_name %><br />
          <%= f.text_field :company_name, autofocus: true, :placeholder => 'company name' %></div>

          <div class="label"><%= f.label :company_address %><br />
          <%= f.text_field :company_address, autofocus: true, :placeholder => 'company address' %></div>

          <div class="label"><%= f.label :country %><br />
          <%= f.text_field :country, autofocus: true, :placeholder => 'country' %></div>

          <div class="label"><%= f.label :user_name %><br />
          <%= f.text_field :user_name, autofocus: true, :placeholder => 'user name' %></div>

          <div class="label"><%= f.label :phone_number %><br />
          <%= f.text_field :phone_number, autofocus: true, :placeholder => 'phone number' %></div>

          <div class="label"><%= f.label :item_type %><br />
          <%= f.text_field :item_type, autofocus: true, :placeholder => 'item type' %></div>





          <div class="label"><%= f.label :email %><br />
          <%= f.email_field :email, autofocus: true, :placeholder => 'email' %></div>

          <div class="label"><%= f.label :password %> <% if @validatable %><i>(<%= @minimum_password_length %> characters minimum)</i><% end %><br />
          <%= f.password_field :password, autocomplete: "off", :placeholder => 'password' %></div>

          <div class="label"><%= f.label :password_confirmation %><br />
          <%= f.password_field :password_confirmation, autocomplete: "off", :placeholder => 'confirmation password' %></div>

          <div class="submit">
          <div class="actions">
          <%= f.submit %>
          </div></div>

          <div class="more_link_box">
          <strong>already has your account</strong>
          <%= render "brands/shared/links" %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
 </div>
</div>

